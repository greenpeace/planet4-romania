{
	"name": "greenpeace/planet4-romania",
	"description": "Greenpeace P4 Romania",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-child-theme-romania": "0.*",
		"greenpeace/planet4-plugin-migrated-urls": "0.0.1",
		"wpackagist-plugin/wp-stateless": "2.1.7"
	},
	"scripts": {
		"core:language": "wp language core install ro_RO;wp language core activate ro_RO",
		"install:plugin-wpimport": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wp-all-import-pro_4.5.4.zip",
		"site:custom": [
			"@core:language",
			"@install:plugin-wpimport"
		]
	}
}
